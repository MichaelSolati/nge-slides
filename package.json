{
  "name": "nge-slides",
  "version": "0.3.0",
  "description": "Sample library of Angular Elements components geared for a slideshow",
  "main": "nge-slides.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node build.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release:major": "changelog -M && git add . && git commit -m 'chore(release): major version release' && npm version major",
    "release:minor": "changelog -m && git add . && git commit -m 'chore(release): minor version release' && npm version minor",
    "release:patch": "changelog -p && git add . && git commit -m 'chore(release): patch version release' && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelSolati/nge-slides.git"
  },
  "keywords": [
    "angular elements",
    "custom elements",
    "web components",
    "slideshow"
  ],
  "author": "Michael Solati <mkslt04@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelSolati/nge-slides/issues"
  },
  "homepage": "https://github.com/MichaelSolati/nge-slides#readme",
  "files": [
    "CHANGELOG.md",
    "LICENSE.md",
    "nge-slides.js",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@angular/animations": "~11.0.4",
